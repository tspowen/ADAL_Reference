{
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*_themes/**"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "globalMetadata": {
      "breadcrumb_path": "/CapsToOpsTest_EMS/v1/toc.json"
    },
    "template": [
      "docs.html"
    ],
    "dest": "_site"
  },
  "render": {
    "theme": "hyde",
    "dest": "_output"
  },
  "basepath": "/AAD"
}